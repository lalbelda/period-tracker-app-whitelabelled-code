{
  "name": "@period-tracker-app/oky-cms",
  "version": "0.0.1",
  "main": "dist/index.js",
  "private": true,
  "description": "Oky CMS, with Express, typescript, TypeORM and ejs",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express-session": "^1.17.4",
    "@types/jquery": "^3.5.10",
    "@types/node": "^17.0.2",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "firebase": "^9.6.1",
    "firebase-admin": "^10.0.1",
    "i18n": "^0.13.3",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.41"
  },
  "scripts": {
    "build": "tsc && yarn run copy-static-assets",
    "clean": "shx rm -rf dist/*",
    "compile": "yarn run build",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "serve": "NODE_PATH=./dist node dist/src/index.js",
    "start": "nodemon --legacy-watch -e ts --exec ts-node --files src/index",
    "migrate-up": "ts-node ../../node_modules/typeorm/cli -f /ormconfig.ts migration:run",
    "projections": "ts-node ./src/projections/stale-data-projection.ts"
  }
}
